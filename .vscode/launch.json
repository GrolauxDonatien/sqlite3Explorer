{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Electron SQLite3 Explorer",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
      "windows": {
        "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
      },
      "args": [
        "."
      ],
      "cwd": "${workspaceRoot}"
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch MSI Builder",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\build_installer.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "test in node.js",
      "program": "${workspaceRoot}/web-app/backend/test.js"
    }
  ]
}